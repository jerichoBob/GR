Cell[TextData[{
 StyleBox["This Notebook computes the Christoffel Symbols from a given metric \
",
  FontFamily->"Helvetica Neue",
  FontWeight->"Regular"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["g", "\[Mu]\[Nu]"], TraditionalForm]],ExpressionUUID->
  "b9b08bf1-cc1e-4e68-a4bf-e8969d19b77f"],
 StyleBox[".\nThe Christoffel Symbols or affine connection are given by:\n",
  FontFamily->"Helvetica Neue",
  FontWeight->"Regular"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "\[Mu]\[Nu]"], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{
      SuperscriptBox["g", "\[Lambda]\[Sigma]"], "(", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Mu]"], " ", 
        SubscriptBox["g", "\[Sigma]\[Nu]"]}], "+", 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Nu]"], " ", 
        SubscriptBox["g", "\[Sigma]\[Mu]"]}], "-", 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Sigma]"], " ", 
        SubscriptBox["g", "\[Mu]\[Nu]"]}]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"e20aa29f-340e-43f6-a649-a44eff9156b5"],
 StyleBox[",\n ( ",
  FontFamily->"Helvetica Neue",
  FontWeight->"Regular"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[PartialD]", 
    RowBox[{"\[Alpha]", "  "}]], TraditionalForm]],ExpressionUUID->
  "5970b182-a163-47df-82bc-af4e7d67f674"],
 StyleBox["stands for the partial derivative ",
  FontFamily->"Helvetica Neue",
  FontWeight->"Regular"],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PartialD]", 
    RowBox[{"/", 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "\[Alpha]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"c4813e1a-130e-4838-9fff-c8781a6594a4"],
 StyleBox[")\nThe user must input the covariant components of the metric \
tensor ",
  FontFamily->"Helvetica Neue",
  FontWeight->"Regular"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["g", "\[Mu]\[Nu]"], TraditionalForm]],ExpressionUUID->
  "0a4c4ebf-0e06-4db7-8c2a-30c16e0f4750"],
 StyleBox[" by editing the relevant input line in this Mathematica notebook. \
All the components computed are in the coordinate basis ",
  FontFamily->"Helvetica Neue",
  FontWeight->"Regular"],
 StyleBox["in which the metric was specified. It works for all finite \
dimensions.\n",
  FontFamily->"Helvetica Neue",
  FontWeight->"Regular",
  FontVariations->{"CompatibilityType"->0}]
}], "Text",
 CellChangeTimes->{{3.7533635083665457`*^9, 
  3.75336353034903*^9}},ExpressionUUID->"023a459f-dce5-431d-a846-\
2eba82794cd4"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ChristoffelSymbols", "[", 
    RowBox[{"metric_", ",", "coord_"}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"inversemetric", ",", "n", ",", " ", "list"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"n", " ", "=", " ", 
       RowBox[{"Length", "[", "coord", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"inversemetric", " ", ":=", " ", 
       RowBox[{"Simplify", "[", 
        RowBox[{"Inverse", "[", "metric", "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Christoffel", "=", "\[IndentingNewLine]", 
       RowBox[{"Simplify", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "/", "2"}], ")"}], "*", 
           RowBox[{"Sum", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"inversemetric", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "s"}], "]"}], "]"}], ")"}], "*", 
              "\[IndentingNewLine]", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"D", "[", 
                 RowBox[{
                  RowBox[{"metric", "[", 
                   RowBox[{"[", 
                    RowBox[{"s", ",", "j"}], "]"}], "]"}], ",", 
                  RowBox[{"coord", "[", 
                   RowBox[{"[", "k", "]"}], "]"}]}], " ", "]"}], "+", 
                "\[IndentingNewLine]", 
                RowBox[{"D", "[", 
                 RowBox[{
                  RowBox[{"metric", "[", 
                   RowBox[{"[", 
                    RowBox[{"s", ",", "k"}], "]"}], "]"}], ",", 
                  RowBox[{"coord", "[", 
                   RowBox[{"[", "j", "]"}], "]"}]}], " ", "]"}], "-", 
                "\[IndentingNewLine]", 
                RowBox[{"D", "[", 
                 RowBox[{
                  RowBox[{"metric", "[", 
                   RowBox[{"[", 
                    RowBox[{"j", ",", "k"}], "]"}], "]"}], ",", 
                  RowBox[{"coord", "[", 
                   RowBox[{"[", "s", "]"}], "]"}]}], " ", "]"}]}], ")"}]}], 
             ",", "\[IndentingNewLine]", 
             RowBox[{"{", 
              RowBox[{"s", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}], " ", "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"list", " ", ":=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"UnsameQ", "[", 
            RowBox[{
             RowBox[{"Christoffel", "[", 
              RowBox[{"[", 
               RowBox[{"i", ",", "j", ",", "k"}], "]"}], "]"}], ",", "0"}], 
            "]"}], ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"ToString", "[", 
              RowBox[{"\[CapitalGamma]", "[", 
               RowBox[{"i", ",", "j", ",", "k"}], "]"}], "]"}], ",", 
             RowBox[{"Christoffel", "[", 
              RowBox[{"[", 
               RowBox[{"i", ",", "j", ",", "k"}], "]"}], "]"}]}], "}"}]}], 
          "]"}], " ", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "n"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "1", ",", "n"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "1", ",", "j"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"a", "=", 
       RowBox[{"Style", "[", 
        RowBox[{"\"\<Christoffel Symbolds:\>\"", ",", " ", 
         RowBox[{"FontWeight", "\[Rule]", "Bold"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"TableForm", "[", 
       RowBox[{
        RowBox[{"Partition", "[", 
         RowBox[{
          RowBox[{"DeleteCases", "[", 
           RowBox[{
            RowBox[{"Flatten", "[", "list", "]"}], ",", "Null"}], "]"}], ",", 
          "2"}], "]"}], ",", 
        RowBox[{"TableSpacing", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"2", ",", "2"}], "}"}]}], ",", 
        RowBox[{"TableHeadings", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", "}"}], ",", 
           RowBox[{"{", "a", "}"}]}], "}"}]}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{
